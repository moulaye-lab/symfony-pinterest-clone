{% extends 'base.html.twig' %}

{% block title %}Find all pins {% endblock %}

{% block body %}
    <h1> {{ pluralize (pins | length, 'Pin', 'Pins') }} </h1>
    
    {% for pin in pins %}
    
        <article>
            <h1><a href="#">{{pin.title}}</h1>
            <p>{{pin.description }}</p>
        </article>
    {% endfor %}
{% endblock %}
